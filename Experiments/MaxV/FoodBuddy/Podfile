target 'FoodBuddy' do
  use_frameworks!

  # Pods for FoodBuddy
  pod "MBCircularProgressBar"

end

post_install do 
  |installer|
  installer.pods_project.build_configurations.each do 
    |config|
    config.build_settings.delete('CODE_SIGNING_ALLOWED')
    config.build_settings.delete('CODE_SIGNING_REQUIRED')
  end
end